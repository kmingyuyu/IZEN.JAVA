package ch04.sec08;

public class qqq {

	public static void main(String[] args) {
		 int[][] array2 = {
       	      {95, 86},
       	      {83, 92, 96},
       	      {78, 83, 93, 87, 88}
       	};
       int totalf = 0;
       int totals = 0;
       
       for(int i = 0; i<array2.length; i++) {
       
       	for(int j=0; j<array2[i].length; i++) {
             
       		totalf += array2[i][j];        		
       	}
       }
       
       System.out.println("총합");

	}

}
