package Quiz;

public class MemberService {

	String id;
	String password;
	
	
public static boolean login(String id , String password) {
	    if(id == "홍길동" && password =="12345") {
	    	return true;
	    }
	        return false;
		}
	
public static void logout(String id) {
		System.out.println(id + "님이 로그아웃 되었습니다.");
	}
	
	


}