package korail;

public class ticketing {
    static int personinfo;
	static String[] trainList = {"ktx" , "itx" , "새마을호" , "무궁화호"}  ;
    static String[] startTrain = {"용산" , "광명" , "서울"} ;
	final static String END_TRAIN = "부산";
	static int startday;
}

ticketing(int personinfo , String[] trainList , String[] startTrain , int startday ) {
	this.personinfo = personinfo;
	this.trainList = trainList;
	this.startTrain = startTrain;
	this.startday = startday;
}

