package ch04.sec08;

public class qqq {

	public static void main(String[] args) {
		 int[][] array = {
       	      {95, 86},
       	      {83, 92, 96},
       	      {78, 83, 93, 87, 88}
       	};
       int totalf = 0;
       int totals = 0;
       double avg =0.0;
       for(int i = 0; i<array.length; i++) {
       	 for(int j=0; j<array[i].length; j++) {
       		totalf += array[i][j];
       		totals +=1;
       	}
       }
       
       avg = totalf/totals;
       
       System.out.println("총합 :" + totalf);
       System.out.println("평균 :" + avg);
       

	}

}
