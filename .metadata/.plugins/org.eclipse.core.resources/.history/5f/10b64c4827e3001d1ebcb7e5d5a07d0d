package ch04.sec08;

public class qqq {

	public static void main(String[] args) {
		 int[][] array = {
       	      {95, 86},
       	      {83, 92, 96},
       	      {78, 83, 93, 87, 88}
       	};
       int totalf = 0;
       int totals = 0;
       
       for(int i = 0; i<array.length; i++) {
       
       	for(int j=0; j<array[i].length; i++) {
       		totalf += array[i][j];        		
       	}
       }
       System.out.println("총합");
       

	}

}
