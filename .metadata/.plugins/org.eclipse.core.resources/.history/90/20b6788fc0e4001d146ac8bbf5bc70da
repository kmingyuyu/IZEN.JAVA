package ch06.sec14;

public class Car {
//  필드
	
	private int speed;
	private boolean stop;
	
	
//	gtetter 메소드 : private 필드를 리턴해주는 메소드
	public int getSpeed() {
		return speed;
		}
	
//	setter 메소드 : private 필드의 값을 변경해주는 메소드
    public void setSpeed(int speed) {
    	this.speed = speed;
    }	
	
	
	
}


