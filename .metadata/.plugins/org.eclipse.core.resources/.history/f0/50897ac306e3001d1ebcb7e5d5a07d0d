package ch05.sec12;

import java.util.Calendar;

public class WeekExample {

	public static void main(String[] args) {
//		Week 타입 변수 선언
		Week today = null;
		
//		오늘의 요일 얻기
		Calendar cal = Calendar.getInstance();
		int week = cal.get(Calendar.DAY_OF_WEEK);
		
		System.out.println(week);
		

	}

}
