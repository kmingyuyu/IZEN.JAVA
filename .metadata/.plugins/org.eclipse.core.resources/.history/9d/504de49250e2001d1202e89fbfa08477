package ch05.sec05;

public class ChartExample {

	public static void main(String[] args) {
		
		String ssn = "950624 1230123";
		     //index: 012345 
	    char sex = ssn.charAt(7); //인덱스 번호로 해당문자를 구한다.
	    System.out.println(sex);
	    
	    switch (sex) {
		case '1':
		case '3':
			System.out.println("남자");
			break;
		case '2':
		case '4':
			System.out.println("여자");
		default:
			break;
		}
	

	}

}
