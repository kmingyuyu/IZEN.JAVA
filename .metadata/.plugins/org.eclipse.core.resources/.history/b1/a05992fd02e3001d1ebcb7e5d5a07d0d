package ch05.sec10;

public class AdvancedForExample {

	public static void main(String[] args) {
          
		
		int[] score = {95,71,84,93,87};
//		평균점수 구하기
		int sum=0;
		for(int scores : score) {
			sum += scores;
			System.out.println(scores);
		}
		
		double avg = (double) sum/score.length;
		System.out.println("평균: "sum);
	}

}
