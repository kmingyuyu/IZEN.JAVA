package Quiz;

public class q0427sec01 {

		
		
		static String name ;
        static String id ;
        static String password ;
        static int age ;
       
  static {
        name = "이름"; 
        id="아이디"; 
        password="패스워드"; 
        age = 5 ; 
         }

  
  q0427sec01(String x, String y) {
	  name = x;
	  id = y;
  };
  
  
  
	}

